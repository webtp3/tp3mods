                            <config>
                                <type>input</type>
                                <default>-20</default>
                                <eval>trim</eval>
                                <range>
                                    <lower>-100</lower>
                                    <upper>100</upper>
                                </range>
                                <wizards>
                                    <slider>
                                        <type>slider</type>
                                        <step>5</step>
                                        <width>200</width>
                                    </slider>
                                </wizards>
                            </config>
                        </TCEforms>
                    </speed>
                </el>
            </ROOT>
